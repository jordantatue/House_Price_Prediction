    -Method POST `
    -Headers @{ "Content-Type" = "application/json" } `
    -Body '{"MedInc":12.5,"HouseAge":15,"AveRooms":7,"AveBedrms":1.2,"Population":800,"AveOccup":2.5,"Latitude":37.5,"Longitude":-122.3}'

